{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": true,
        "allowJs": true,
        "strict": true,
        "module": "ESNext",
        "target": "es5",
        "jsx": "react",
        "baseUrl":"src/",
        "paths": {
            "@commons/*": ["commons/*"],
        },
    },
    "include": [
        "src/**/*",
    ],
    "exclude": [
        "node_modules",
    ],
}
